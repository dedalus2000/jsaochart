
{
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "no-empty": 1,
    "no-unexpected-multiline": 1,
    "no-unreachable": 1,
    "no-unsafe-negation": 1,
    "valid-typeof": 1,
    "no-eq-null": 1,
    "no-multi-spaces": 1,

    "no-undef-init":2,
    "no-shadow":2,
    "wrap-iife":2,
    "vars-on-top":2,
    "no-redeclare":2,
    "no-lone-blocks":2,
    "eqeqeq":2,
    "curly":2,
    "block-scoped-var":1,
    "use-isnan":1,
    "no-undef": 1,
    "no-unused-vars": 1,
    "no-use-before-define": 1,
    "no-func-assign": 1,
    "indent": ["error", 2],
    "semi": 1
  }
}
